{% extends 'front/layout.html.twig' %}

{% block title %}{{ 'Accueil - '~ parent() }}{% endblock %}

{% block content %}
                {% block hero_wrapper %}
                    <section class="hero-wrapper overflow-hidden">
                        {% block banner %}
                            <div class="overlay"></div><!-- end overlay -->
                            {{ render(url('app_banner')) }}
                        {% endblock %}
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="hero-heading text-center">
                                        <div class="section-heading">
                                            <h2 class="sec__title cd-headline slide">
                                                Qui Recherchez-vous ?
                                                <span class="cd-words-wrapper py-0">
                                                {% for category in my_app.categoriesProPopular %}
                                                    <b class="{{ loop.first ? "is-visible" : "" }}">{{ category.name|e }}</b>
                                                {% endfor %}
                                            </span>
                                            </h2>
                                            <p class="sec__desc">
                                                Trouver des professionnels qualifiés pour vous rendre le meilleur service.
                                            </p>
                                        </div>
                                    </div><!-- end hero-heading -->
                                    <div class="main-search-input">
                                        <div class="main-search-input-item">
                                            <form action="#" class="form-box">
                                                <div class="form-group mb-0">
                                                    <span class="la la-search form-icon"></span>
                                                    <input class="form-control" type="search" placeholder="De quoi avez-vous besoin ?">
                                                </div>
                                            </form>
                                        </div><!-- end main-search-input-item -->
                                        <div class="main-search-input-item user-chosen-select-container">
                                            <select class="user-chosen-select">
                                                <option value="0">Selectionner une categorie</option>
                                                <option value="1">Shops</option>
                                            </select>
                                        </div><!-- end main-search-input-item -->
                                        <div class="main-search-input-item user-chosen-select-container">
                                            <select class="user-chosen-select">
                                                <option value="0">Selectionner un service</option>
                                                <option value="1">Shops</option>
                                            </select>
                                        </div><!-- end main-search-input-item -->
                                        <div class="main-search-input-item">
                                            <button class="theme-btn gradient-btn border-0 w-100" type="submit"><i class="la la-search mr-2"></i>Rechercher</button>
                                        </div><!-- end main-search-input-item -->
                                    </div><!-- end main-search-input -->
                                    <div class="highlighted-categories">
                                        <div class="d-flex align-items-end justify-content-center highlight-search-desc py-5 text-center">
                                            <img src="{{ asset('assets/front/images/arrow-shape.png') }}" alt="" class="mr-5 cat-arrow-icon">
                                            <h5 class="highlighted__title">Just looking around? Let us suggest you <br> something hot & happening!</h5>
                                        </div>
                                        {% if categories_pro_popular|length > 0 %}
                                            <div class="highlight-lists d-flex justify-content-center mt-4">
                                                {% for category in my_app.categoriesProPopular|slice(0, 6) %}
                                                    <div class="hero-category-item">
                                                        <a href="{{ path("app_category_pro", {"slug" : category.slug}) }}" class="d-block hero-cat-link hover-y">
                                                            <span class="icon-element bg-1 mx-auto"><i class="la la-building-o"></i></span>
                                                            {{ category.name|e }}
                                                        </a>
                                                    </div>
                                                {% endfor %}
                                            </div>
                                        {% endif %}
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="svg-bg">
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1000 100" preserveAspectRatio="none">
                                <path fill="#F5F7FC" class="elementor-shape-fill" d="M500,97C126.7,96.3,0.8,19.8,0,0v100l1000,0V1C1000,19.4,873.3,97.8,500,97z"></path>
                            </svg>
                        </div>
                    </section><!-- end hero-wrapper -->
                {% endblock %}

                {% block vip_home %}
                    <section class="card-area section-padding vip">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="section-heading text-center">
                                        <h2 class="sec__title">Professionnels VIP</h2>
                                        <p class="sec__desc">
                                            On aime, on adore travailler avec ceux qui savent ce qu'ils font.
                                        </p>
                                    </div><!-- end section-heading -->
                                </div><!-- end col-lg-12 -->
                            </div><!-- end row -->
                            <div class="row padding-top-60px">
                            {% for i in 1..4 %}
                                <div class="col-lg-3 responsive-column">
                                    <div class="card-item">
                                        <div class="card-image">
                                            <a href="#" class="d-block">
                                                <img src="{{ asset('assets/front/images/img-loading.png') }}" data-src="{{ asset('assets/front/images/img1.jpg') }}" class="card__img lazy" alt="">
                                            </a>
                                        </div>
                                        <div class="card-content">
                                            <a href="#" class="user-thumb d-inline-block" data-toggle="tooltip" data-placement="top" title="TechyDevs">
                                                <img src="{{ asset('assets/front/images/photoshop.png') }}" alt="author-img">
                                            </a>
                                            <h4 class="card-title pt-3">
                                                <a href="#">Roma’s Ristorante</a>
                                                <i class="la la-check-circle ml-1" data-toggle="tooltip" data-placement="top" title="Vérifié"></i>
                                            </h4>
                                            <p class="card-sub"><a href="#"><i class="la la-map-marker mr-1 text-color-2"></i>Bishop Avenue, New York</a></p>
                                            <ul class="listing-meta d-flex align-items-center">
                                                <li class="d-flex align-items-center">
                                                    <span class="rate flex-shrink-0">4.7</span>
                                                    <span class="rate-text">5 avis</span>
                                                </li>
                                            </ul>
                                            <ul class="info-list padding-top-20px">
                                                <li><span class="la la-calendar-check-o icon"></span>
                                                    Opened a few days ago
                                                </li>
                                            </ul>
                                        </div>
                                    </div><!-- end card-item -->
                                </div><!-- end col-lg-4 -->
                            {% endfor %}
                            </div><!-- end row -->
                            <div class="more-link-wrap text-right">
                                <a href="#" class="btn-text font-weight-medium">Voir plus de professionnels prêts à vous aider <i class="la la-arrow-right icon"></i></a>
                            </div>
                        </div><!-- end container -->
                    </section><!-- end card-area -->
                {% endblock %}

                {% block category_area %}
                    <section class="category-area bg-gray arrow-down-shape position-relative section-padding">
                        <div class="container">
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="section-heading text-center">
                                        <h2 class="sec__title">Catégories populaires</h2>
                                    </div>
                                </div>
                            </div>
                            <div class="row mt-5">
                            {% for category in my_app.categoriesProPopular|slice(0, 8) %}
                                <div class="col-lg-3 responsive-column">
                                    <div class="category-item overflow-hidden">
                                        <img src="{{ asset('assets/front/images/img-loading.png') }}" data-src="{{ vich_uploader_asset(category, 'iconFile')|imagine_filter('category') }}" alt="{{ category.name|e }}" class="lazy cat-img">
                                        <div class="category-content d-flex align-items-center justify-content-center">
                                            <a href="#" class="category-link d-flex flex-column justify-content-center w-100 h-100">
                                                <div class="icon-element mb-3 mx-auto">
                                                    <span class="la la-dumbbell"></span>
                                                </div>
                                                <div class="cat-content">
                                                    <h4 class="cat__title mb-3">{{ category.name|e }}</h4>
                                                    <span class="badge">{{ category.professionals|length }} Professionnels</span>
                                                </div>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            {% endfor %}
                            </div><!-- end row -->
                            <div class="more-btn-box pt-3 text-center">
                                <a href="#" class="btn-gray hover-scale-2">parcourir toutes les catégories <i class="la la-arrow-right ml-2"></i></a>
                            </div>
                        </div><!-- end container -->
                    </section><!-- end category-area -->
                {% endblock %}

