                {% if pageCount > 1 %}
                    <div class="pagination-wrapper d-inline-block">
                        <div class="section-pagination">
                            <nav aria-label="Page navigation">
                                <ul class="pagination flex-wrap justify-content-center">
                                    {% if previous is defined %}
                                        <li class="page-item">
                                            <a class="page-link" href="{{ path(route, query|merge({(pageParameterName): previous})) }}" aria-label="Previous">
                                                <span aria-hidden="true"><i class="la la-angle-left"></i></span>
                                                <span class="sr-only">&laquo;&nbsp;{{ 'label_previous'|trans({}, 'KnpPaginatorBundle') }}</span>
                                            </a>
                                        </li>
                                    {% else %}
                                        <li class="page-item">
                                            <a class="page-link" href="javascript:void(0)">
                                                <span aria-hidden="true"><i class="la la-angle-left"></i></span>
                                                <span class="sr-only">&laquo;&nbsp;{{ 'label_previous'|trans({}, 'KnpPaginatorBundle') }}</span>
                                            </a>
                                        </li>
                                    {% endif %}
                                    {% if startPage > 1 %}
                                        <li class="page-item"><a class="page-link" href="{{ path(route, query|merge({(pageParameterName): 1})) }}">1</a></li>
                                        {% if startPage == 3 %}
                                            <li class="page-item"><a class="page-link" href="{{ path(route, query|merge({(pageParameterName): 2})) }}">2</a></li>
                                        {% elseif startPage != 2 %}
                                            <li class="page-item"><a class="page-link" href="javascript:void(0)">&hellip;</a></li>
                                        {% endif %}
                                    {% endif %}
                                    {% for page in pagesInRange %}
                                        {% if page != current %}
                                            <li class="page-item"><a class="page-link" href="{{ path(route, query|merge({(pageParameterName): page})) }}">{{ page }}</a></li>
                                        {% else %}
                                            <li class="page-item"><a class="page-link page-link-active" href="javascript:void(0)">{{ page }}</a></li>
                                        {% endif %}
                                    {% endfor %}
                                    {% if pageCount > endPage %}
                                        {% if pageCount > (endPage + 1) %}
                                            {% if pageCount > (endPage + 2) %}
                                                <li class="page-item"><a class="page-link" href="!#">&hellip;</a></li>
                                            {% else %}
                                                <li class="page-item">
                                                    <a class="page-link" href="{{ path(route, query|merge({(pageParameterName): (pageCount - 1)})) }}">{{ pageCount -1 }}</a>
                                                </li>
                                            {% endif %}
                                        {% endif %}
                                        <li class="page-item">
                                            <a class="page-link" href="{{ path(route, query|merge({(pageParameterName): pageCount})) }}">{{ pageCount }}</a>
                                        </li>
                                    {% endif %}
                                    {% if next is defined %}
                                        <li class="page-item">
                                            <a class="page-link" href="{{ path(route, query|merge({(pageParameterName): next})) }}" aria-label="Next">
                                                <span aria-hidden="true"><i class="la la-angle-right"></i></span>
                                                <span class="sr-only">{{ 'label_next'|trans({}, 'KnpPaginatorBundle') }}&nbsp;&raquo;</span>
                                            </a>
                                        </li>
                                    {% else %}
                                        <li class="page-item">
                                            <a class="page-link" href="javascript:void(0)">
                                                <span aria-hidden="true"><i class="la la-angle-right"></i></span>
                                                <span class="sr-only">{{ 'label_next'|trans({}, 'KnpPaginatorBundle') }}&nbsp;&raquo;</span>
                                            </a>
                                        </li>
                                    {% endif %}
                                </ul>
                            </nav>
                        </div>
                    </div>
                {% endif %}